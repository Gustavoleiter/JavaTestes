package br.com.terceiroprojeto.teste;

public class Teste {
 
	public  static void main(String [] args) {
		
		
		String teste = "37";
		String teste1 = "20";
		String testeResultado = teste + teste1;
		
		System.out.println(testeResultado);
		
		String teste2 = "37";
		String teste3 = "20";
		int teste2Convertido = Integer.parseInt(teste2);
		int teste3Convertido = Integer.parseInt(teste3);
		int resultadoTesteConvertido = teste3Convertido + teste2Convertido;
		
		System.out.println("Esse foi o resultado = " + resultadoTesteConvertido);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
	

}
